{
  "writeFiles": [
    {
      "name": ".editorconfig",
      "value": [
        "#https://editorconfig.org/",
        "",
        "root = true",
        "",
        "[*]",
        "charset = utf-8",
        "end_of_line = lf",
        "indent_style = space",
        "trim_trailing_whitespace = true",
        "",
        "[*.md]",
        "trim_trailing_whitespace = false"
      ]
    },
    {
      "name": ".gitignore",
      "value": ["node_modules", "build/", ".DS_Store", ".env", ".env.*", ".vscode"]
    },
    {
      "name": ".eslintrc",
      "update": true
    },
    {
      "name": ".eslintignore",
      "value": ["build/", "dist/", "public/", "node_modules/", "**/.cache"]
    },
    {
      "name": "test/makeDirectory/test.md",
      "value": ["#test"]
    }
  ]
}
